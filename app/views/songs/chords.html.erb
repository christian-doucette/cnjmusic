<head>
  <meta property="og:image" content="<%= @song.album.images.first['url'] %>" />
  <meta property="og:title" content="&quot;<%= @song.name %>&quot; Chords" />
</head>

<body>

<h1><%= @song.name %> Chords</h1>

<div class="row">
<% @chords.each_with_index do |chord, index| %>
  <div class="col-3">
  <%= chord %>
  </div>
<% end %>
</div>



</body>
